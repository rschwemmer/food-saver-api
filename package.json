{
  "name": "food-saver-api",
  "version": "1.0.0",
  "description": "yo mama",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs",
    "test": "echo --connectionString=$(cat .env.development | grep DATABASE_URL | awk -F '=' '{print $NF}' | tr -d '\"')",
    "start": "env-cmd -f .env tsup src/index.ts --watch --format cjs --onSuccess 'node dist/index.js'",
    "drizzle:push": "drizzle-kit push:mysql --driver mysql2 --schema src/db/schema/ --connectionString=$(cat .env.development | grep DATABASE_URL | awk -F '=' '{print $NF}' | tr -d '\"')"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.27.0",
    "node": "^20.2.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "drizzle-kit": "^0.19.2",
    "express": "^4.18.2",
    "mysql2": "^3.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "tsup": "^7.0.0"
  }
}